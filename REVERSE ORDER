#include <stdio.h>
int main() {
    int num,q,r;
    printf("enter a 3 digits=");
    scanf("%d",&num);
    q=num;
    while (q>0){
        r=q%10;
        printf("%d\n",r);
        q=q/10;
        r=q%10;
    }
        return 0;
}

#### OUTPUT ####
enter a 3 digits=911 
1
1
9

=== Code Execution Successful ===
